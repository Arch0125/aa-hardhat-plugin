{
  "name": "hh-acc-abs-toolkit",
  "version": "1.0.2",
  "description": "Account abstraction plugin for hardhat",
  "repository": "https://github.com/Arch0125/aa-hardhat-plugin",
  "author": "Nomic Labs LLC",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "homepage": "https://github.com/Arch0125/aa-hardhat-plugin#readme",
  "keywords": [
    "ethereum",
    "smart-contracts",
    "hardhat",
    "hardhat-plugin"
  ],
  "scripts": {
    "lint:fix": "prettier --write 'src/**/*.{js,ts}' 'test/**/*.{js,ts}' && tslint --fix --config tslint.json --project tsconfig.json",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "test": "mocha --exit --recursive 'test/**/*.test.ts'",
    "build": "rm -rf dist && tsc --outDir dist/src/",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build"
