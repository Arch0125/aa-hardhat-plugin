  "files": [
    "dist/src/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cli-color": "^2.0.2",
    "@types/fs-extra": "^5.0.4",
